server {
	listen {{ blog_server_port }};
	server_name {{ blog_server_name }};
	root {{ blog_code_path }};

	location / {
		index index.php;
	}


	location ~ \.php$ {
		fastcgi_pass 127.0.0.1:9000;
		fastcgi_param SCRIPT_FILENAME $document_root$fastcgi_script_name;
		fastcgi_param HTTPS on;
		include fastcgi_params;
	}
}
